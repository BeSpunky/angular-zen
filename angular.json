{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "@bespunky/angular-zen": {
      "root": "projects/bespunky/angular-zen",
      "sourceRoot": "projects/bespunky/angular-zen",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/bespunky/angular-zen/tsconfig.lib.json",
            "project": "projects/bespunky/angular-zen/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/bespunky/angular-zen/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/bespunky/angular-zen/test.ts",
            "tsConfig": "projects/bespunky/angular-zen/tsconfig.spec.json",
            "karmaConfig": "projects/bespunky/angular-zen/karma.conf.js",
            "codeCoverage": true
          },
          "configurations": {
            "production": {
              "browsers": "ChromeHeadless",
              "watch": false
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/bespunky/angular-zen/tsconfig.lib.json",
              "projects/bespunky/angular-zen/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "official-site": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/official-site",
      "sourceRoot": "projects/official-site/src",
      "prefix": "zen",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/official-site",
            "index": "projects/official-site/src/index.html",
            "main": "projects/official-site/src/main.ts",
            "polyfills": "projects/official-site/src/polyfills.ts",
            "tsConfig": "projects/official-site/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/official-site/src/favicon.ico",
              "projects/official-site/src/assets"
            ],
            "styles": [
              "projects/official-site/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/official-site/src/environments/environment.ts",
                  "with": "projects/official-site/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "official-site:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "official-site:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "official-site:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/official-site/src/test.ts",
            "polyfills": "projects/official-site/src/polyfills.ts",
            "tsConfig": "projects/official-site/tsconfig.spec.json",
            "karmaConfig": "projects/official-site/karma.conf.js",
            "assets": [
              "projects/official-site/src/favicon.ico",
              "projects/official-site/src/assets"
            ],
            "styles": [
              "projects/official-site/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/official-site/tsconfig.app.json",
              "projects/official-site/tsconfig.spec.json",
              "projects/official-site/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/official-site/e2e/protractor.conf.js",
            "devServerTarget": "official-site:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "official-site:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "@bespunky/angular-zen"
}
